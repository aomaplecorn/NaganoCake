<h1>Admin::Customers#edit</h1>

<div>
<%= form_with model: [:admin,@customer] do |f| %>
<p>会員ID
<%= @customer.id %></p>
<p>氏名
<%= f.text_field :last_name %>
<%= f.text_field :first_name %></p>
<p>フリガナ
<%= f.text_field :last_name_kana %>
<%= f.text_field :first_name_kana %></p>
<p>郵便番号
<%= f.text_field :postal_code %></p>
<p>住所
<%= f.text_field :address %></p>
<p>電話番号
<%= f.telephone_field :telephone_number %></p>
<p>メールアドレス
<%= f.email_field :email %></p>
<p>会員ステータス
<%= f.radio_button :is_deleted, false %><%= f.label :is_deleted_1, "有効" %>
<%= f.radio_button :is_deleted, true %><%= f.label :is_deleted_2, "無効" %></p>

<%= f.submit "変更を保存" %>

<% end %>
</div>



